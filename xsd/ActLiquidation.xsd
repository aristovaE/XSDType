			<xsd:documentation>Акт об уничтожении</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ActLiquidation_ITEM" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:all>
						<xsd:element name="DOC_ID" type="xsd:integer" nillable="false"/>
