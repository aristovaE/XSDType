						<xsd:element name="ActLiquidation_WasteGoods" type="ActLiquidation_WasteGoods_TABLE" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Отходы, полученные из товаров и (или) упаковки</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ActLiquidation_DocSig" type="ActLiquidation_DocSig_TABLE" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Лицо, присутствующее при уничтожении</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ActLiquidation_GTD" type="ActLiquidation_GTD_TABLE" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Регистрационный номер таможенной декларации</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ActLiquidation_Goods" type="ActLiquidation_Goods_TABLE" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Сведения о товарах, помещенных под таможенную процедуру уничтожения</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:all>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ActLiquidation_WasteGoods_TABLE">
		<xsd:annotation>
			<xsd:documentation>Отходы, полученные из товаров и (или) упаковки</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ActLiquidation_WasteGoods_ITEM" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Отходы, полученные из товаров и (или) упаковки</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:all minOccurs="0">
						<xsd:element name="DOC_ID" type="xsd:integer" nillable="false"/>
						<xsd:element name="GoodsDescription" nillable="true" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Наименование отхода в соответствии с Федеральным классификационным каталогом отходов</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="1000"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="WasteCode" nillable="true" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Код отхода в соответствии с Федеральным классификационным каталогом отходов</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="13"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="Method" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Способ уничтожения</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="32767"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="WasteSign" type="xsd:integer" nillable="true" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Признак происхождения отходов: 0 - товары; 1 - упаковка</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="NewWasteSign" type="xsd:integer" nillable="true" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Признак наличия отходов, образовавшиеся в результате уничтожения: 1 - есть 0 - нет</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="GoodsTNVEDCode" nillable="true" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Код отхода в оответствии с ТН ВЭД ТС</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="10"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="CustomsProc" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Таможенная процедура, под которую планируется помещать отходы</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="GoodsQuantity" nillable="true" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Количество товара в ед.изм. </xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:decimal">
									<xsd:totalDigits value="24"/>
									<xsd:fractionDigits value="6"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="UnitQualifierName" nillable="true" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Название единицы измерения товара</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="13"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="UnitQualifierCode" nillable="true" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Код единицы измерения товара</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="3"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="ActLiquidation_PackingInfo" type="ActLiquidation_PackingInfo_TABLE" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Сведения об упаковке</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="DocumentName" nillable="true" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Наименование документа</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="250"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="DocumentNumber" nillable="true" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Номер документа</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="50"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="DocumentDate" type="xsd:dateTime" nillable="true" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дата документа</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ActLiquidation_GoodsPartsGTD" type="ActLiquidation_GoodsPartsGTD_TABLE" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Сведения о партии товаров</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="Place_PostalCode" nillable="true" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Местонахождение отходов: почтовый индекс</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="9"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="Place_CountryCode" nillable="true" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Местонахождение отходов :  буквенный код страны</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="2"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="Place_CountryName" nillable="true" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Местонахождение отходов: наименование страны</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="40"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="Place_Region" nillable="true" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Местонахождение отходов : область (регион, штат, провинция и т.п.)</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="50"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="Place_City" nillable="true" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Местонахождение отходов : населенный пункт</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="35"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="Place_StreetHouse" nillable="true" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Местонахождение отходов:  улица, номер дома, номер офиса</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="50"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:all>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ActLiquidation_Goods_TABLE">
		<xsd:annotation>
			<xsd:documentation>Сведения о товарах, помещенных под таможенную процедуру уничтожения</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ActLiquidation_Goods_ITEM" minOccurs="0" maxOccurs="unbounded">
