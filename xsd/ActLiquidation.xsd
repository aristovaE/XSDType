<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xsd:complexType name="ActLiquidation_DocSig_TABLE">
		<xsd:annotation>
			<xsd:documentation>Лицо, присутствующее при уничтожении</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ActLiquidation_DocSig_ITEM" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Лицо, присутствующее при уничтожении</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:all minOccurs="0">
						<xsd:element name="DOC_ID" type="xsd:integer" nillable="false"/>
						<xsd:element name="PersonName" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Фамилия</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="150"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="ActLiquidation" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation> Имя</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="150"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="PersonMiddleName" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Отчество</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="150"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="PersonPost" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Должность</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="250"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="IssueDate" type="xsd:dateTime" nillable="true" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дата подписания</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="PersonSign" type="xsd:integer" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Признак лица: 0 - от уполномоченного таможенного органа; 1 - от заявителя (резидента ОЭЗ/декларанта/таможенного представителя); 2 - от организации, осуществляющей уничтожение</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:all>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ActLiquidation_GTD_TABLE">
		<xsd:annotation>
			<xsd:documentation>Регистрационный номер таможенной декларации</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ActLiquidation_GTD_ITEM" minOccurs="0" maxOccurs="unbounded">
