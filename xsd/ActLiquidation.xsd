		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ActLiquidation_GoodsPartsGTD_ITEM" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Отходы, полученные из товаров и (или) упаковки</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:all minOccurs="0">
						<xsd:element name="DOC_ID" type="xsd:integer" nillable="false"/>
