						<xsd:element name="Declarant_KPP" nillable="true" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Декларант: КПП</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="9"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="Declarant_PostalCode" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Декларант: почтовый индекс</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="9"/>
								</xsd:restriction>
