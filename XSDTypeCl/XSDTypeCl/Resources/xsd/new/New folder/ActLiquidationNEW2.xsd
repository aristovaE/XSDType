              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="NewWasteSign" nillable="true" type="xsd:integer">
              <xsd:annotation>
                <xsd:documentation>Признак наличия отходов, образовавшиеся в результате уничтожения: 1 - есть 0 - нет</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="GoodsTNVEDCode" nillable="true">
