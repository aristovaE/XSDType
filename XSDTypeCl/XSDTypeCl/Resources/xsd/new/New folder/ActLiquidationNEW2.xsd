                <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="10" />
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="0" name="CustomsProc" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Таможенная процедура, под которую планируется помещать отходы</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="GoodsQuantity" nillable="true">
              <xsd:annotation>
                <xsd:documentation>Количество товара в ед.изм. </xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:decimal">
                  <xsd:totalDigits value="24" />
                  <xsd:fractionDigits value="6" />
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="0" name="UnitQualifierName" nillable="true">
              <xsd:annotation>
                <xsd:documentation>Название единицы измерения товара</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="13" />
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="0" name="UnitQualifierCode" nillable="true">
              <xsd:annotation>
                <xsd:documentation>Код единицы измерения товара</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="3" />
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="0" name="ActLiquidation_PackingInfo" type="ActLiquidation_PackingInfo_TABLE">
              <xsd:annotation>
                <xsd:documentation>Сведения об упаковке</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="DocumentName" nillable="true">
              <xsd:annotation>
                <xsd:documentation>Наименование документа</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="250" />
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="0" name="DocumentNumber" nillable="true">
              <xsd:annotation>
                <xsd:documentation>Номер документа</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="50" />
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="0" name="DocumentDate" nillable="true" type="xsd:dateTime">
              <xsd:annotation>
                <xsd:documentation>Дата документа</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="ActLiquidation_GoodsPartsGTD" type="ActLiquidation_GoodsPartsGTD_TABLE">
              <xsd:annotation>
                <xsd:documentation>Сведения о партии товаров</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Place_PostalCode" nillable="true">
              <xsd:annotation>
                <xsd:documentation>Местонахождение отходов: почтовый индекс</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="9" />
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="0" name="Place_CountryCode" nillable="true">
              <xsd:annotation>
                <xsd:documentation>Местонахождение отходов :  буквенный код страны</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="2" />
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="0" name="Place_CountryName" nillable="true">
              <xsd:annotation>
                <xsd:documentation>Местонахождение отходов: наименование страны</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="40" />
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="0" name="Place_Region" nillable="true">
              <xsd:annotation>
                <xsd:documentation>Местонахождение отходов : область (регион, штат, провинция и т.п.)</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="50" />
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="0" name="Place_City" nillable="true">
              <xsd:annotation>
                <xsd:documentation>Местонахождение отходов : населенный пункт</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="35" />
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="0" name="Place_StreetHouse" nillable="true">
              <xsd:annotation>
                <xsd:documentation>Местонахождение отходов:  улица, номер дома, номер офиса</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="50" />
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ActLiquidation_Goods_TABLE">
