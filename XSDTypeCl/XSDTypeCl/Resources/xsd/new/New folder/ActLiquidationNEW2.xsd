              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="0" name="WasteCode" nillable="true">
              <xsd:annotation>
                <xsd:documentation>Код отхода в соответствии с Федеральным классификационным каталогом отходов</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
